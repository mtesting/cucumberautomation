<jaxws:bindings xmlns:jaxws="http://java.sun.com/xml/ns/jaxws"
                xmlns:xsd="http://www.w3.org/2001/XMLSchema-instance"
                xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/"
                xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
                xsd:schemaLocation="http://java.sun.com/xml/ns/jaxws http://www.oracle.com/webfolder/technetwork/jsc/xml/ns/jaxws/wsdl_customizationschema_2_0.xsd
                            http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd">

    <jaxws:bindings node="wsdl:definitions">
        <jaxws:package name="ats.betting.trading.att.ws.scenario"/>
    </jaxws:bindings>

    <jaxws:bindings node="wsdl:definitions/wsdl:types/xsd:schema[@targetNamespace='http://testscenario.amelco.co.uk']">
        <jaxb:globalBindings generateElementProperty="false"/>
        <jaxb:schemaBindings>
            <jaxb:package name="ats.betting.trading.att.ws.scenario.dto"/>
        </jaxb:schemaBindings>

    </jaxws:bindings>

</jaxws:bindings>